alias personal="~/dropbox/uni/personalmanagement"

# Shortcuts to directories or files
alias cl="cd; clear"
alias c="clear"
alias dotfiles="~/dotfiles"
alias cheatsheets="~/cheatsheets"
alias scripts="~/scripts"
alias dl="~/Downloads"
alias dt="~/Desktop"
alias db="~/Dropbox"
alias dropbox="~/Dropbox"
alias doc="~/Documents"
alias wallpaper="~/Dropbox/Wallpaper"
alias icons="~/icons"
